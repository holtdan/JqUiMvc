@model JqUiMvc.Models.TaskViewModel
@{
    ViewBag.Title = Model.CurrStep.Name;
}

@using (Html.BeginForm("SaveAndContinue", "Home", FormMethod.Post, new { @class = "form-horizontal", role = "form", area = "" }))
{
    @Html.HiddenFor(m => m.CurrStep.View)
    @Html.EnumDropDownListFor(m => m.CurrStep.State, new { @class = "form-control" })
    @:View = @Model.CurrStep.View
@:State = @Model.CurrStep.State
}
